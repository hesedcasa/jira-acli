{
  "name": "jira-acli",
  "description": "CLI for Jira API interaction",
  "version": "0.1.0",
  "author": "Hesed",
  "bin": {
    "jira-acli": "./bin/run.js"
  },
  "bugs": "https://github.com/hesedcasa/jira-acli/issues",
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@oclif/core": "^4",
    "@oclif/plugin-commands": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-update": "^4.7.18",
    "@oclif/plugin-version": "^2",
    "@toon-format/toon": "^2.1.0",
    "fs-extra": "^11.3.3",
    "jira.js": "^5.3.0",
    "marklassian": "^1.1.0",
    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/sinon": "^21.0.0",
    "@types/turndown": "^5.0.6",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "esmock": "^2.7.3",
    "mocha": "^10",
    "oclif": "^4",
    "prettier": "^3.8.1",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "ts-prune": "^0.10.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/hesedcasa/jira-acli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "jira-acli",
    "dirname": "jira-acli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version",
      "@oclif/plugin-commands",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {},
    "update": {
      "autoupdate": {
        "debounce": 7
      }
    }
  },
  "repository": "hesedcasa/jira-acli",
  "scripts": {
    "build": "shx rm -rf dist tsconfig.tsbuildinfo && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "find-deadcode": "ts-prune --ignore '(run|default)'",
    "format": "eslint --cache --fix --quiet . && prettier --write --cache .",
    "pre-commit": "npm run format && npm run find-deadcode"
  },
  "types": "dist/index.d.ts"
}
